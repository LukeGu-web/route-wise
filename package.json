{
  "name": "route-wise",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "prebuild": "expo prebuild",
    "build:ios": "eas build --platform ios",
    "build:device": "eas build --profile development --platform ios",
    "build:simulator": "eas build -p ios --profile development-simulator",
    "submit:ios": "eas build -p ios --auto-submit",
    "package:check": "expo install --check",
    "package:update": "yarn upgrade-interactive",
    "eas-build-pre-install": "corepack enable",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/dialog": "^1.1.0",
    "@rn-primitives/dropdown-menu": "^1.1.0",
    "@rn-primitives/portal": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/switch": "^1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@tanstack/react-query": "^5.67.2",
    "axios": "^1.8.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.13",
    "expo": "~52.0.38",
    "expo-dev-client": "~5.0.14",
    "expo-linking": "~7.0.4",
    "expo-localization": "~16.0.1",
    "expo-navigation-bar": "~4.0.7",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.20",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "i18next": "^24.2.3",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.7",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~18.3.12",
    "@types/react-native-get-random-values": "^1",
    "typescript": "^5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true,
  "packageManager": "yarn@4.7.0"
